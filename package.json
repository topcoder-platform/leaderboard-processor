    "nock": "^11.4.0",
    "nyc": "^14.1.1",
    "standard": "^12.0.1"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "config": "^1.21.0",
    "get-parameter-names": "^0.3.0",
    "joi": "^14.0.0",
    "lodash": "^4.17.11",
    "mongoose": "^5.13.9",
    "no-kafka": "^3.2.4",
    "superagent": "^3.8.3",
    "tc-core-library-js": "appirio-tech/tc-core-library-js.git#v2.6",
    "topcoder-healthcheck-dropin": "^1.0.3",
    "winston": "^3.1.0"
  },
  "engines": {
    "node": "10.x"
  },
  "standard": {
